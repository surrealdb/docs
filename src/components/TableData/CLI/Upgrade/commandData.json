{
  "usageHeading": "Upgrade to the latest stable version",
  "usage": "surreal upgrade [OPTIONS]",
  "args": "[PATTERN] Glob pattern for the files to validate [default: **/* .surql]",
  "options": [
    {
      "option": "--nightly ",
      "description": "Install the latest nightly version"
    },
    {
      "option": "--version <VERSION> ",
      "description": "Install a specific version"
    },
    {
      "option": "--dry-run ",
      "description": "Don't actually replace the executable"
    },
    {
      "option": "-h, --help",
      "description": "Print help"
    }
  ]
}
