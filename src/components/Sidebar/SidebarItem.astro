---
import RenderSidebarGroup, {
    type SidebarGroup,
} from './RenderSidebarGroup.astro';
import RenderSidebarLink, { type SidebarLink } from './RenderSidebarLink.astro';

export type SidebarItem = SidebarLink | SidebarGroup;
export type Props = SidebarItem;
---

{
    Astro.props.items 
        ? <RenderSidebarGroup {...Astro.props} /> 
        : <RenderSidebarLink {...Astro.props} />
}